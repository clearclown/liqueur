{
  "name": "@liqueur/artifact-store",
  "version": "0.1.0",
  "description": "Artifact persistence layer for LiquidView schemas",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src tests --ext .ts",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "docs": "typedoc src/index.ts --out docs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ablaze/liqueur.git",
    "directory": "packages/artifact-store"
  },
  "bugs": {
    "url": "https://github.com/ablaze/liqueur/issues"
  },
  "homepage": "https://github.com/ablaze/liqueur#readme",
  "keywords": [
    "artifact",
    "persistence",
    "storage",
    "schema"
  ],
  "author": "Project Liquid Contributors",
  "license": "MIT",
  "dependencies": {
    "@liqueur/protocol": "file:../protocol"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "prettier": "^3.1.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "engines": {
    "node": ">=20.0.0"
  }
}

{
  "name": "@liqueur/artifact-store",
  "version": "0.1.0",
  "description": "Artifact persistence layer for LiquidView schemas",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src tests --ext .ts"
  },
  "keywords": [
    "artifact",
    "persistence",
    "storage",
    "schema"
  ],
  "author": "Project Liquid Contributors",
  "license": "MIT",
  "dependencies": {
    "@liqueur/protocol": "file:../protocol"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitest/coverage-v8": "^3.0.3",
    "eslint": "^8.55.0",
    "typescript": "^5.7.3",
    "vitest": "^3.0.3"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
